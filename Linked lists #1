
class Node:
    def __init__(self,value,next = None):

        self.value = value
        self.next = next

class Lista:

    def __init__(self,size, head=None):

        self.head = head
        self.size = size

    def add_head(self,n):
        if self.head is None:
            self.head = n
            self.size += 1
        else: 
            old = self.head
            self.head = n
            n.next = old
            self.size = self.size + 1
    def traverse():
        pass 
        
    def insert_at(self, index, value):
        if index >= self.size:
            return "Fuera de rango bro..."
        else: 
            pass 




e1 = Node(1)
e2 = Node(2)
e3 = Node(50)
e4 = Node(50)
linkedlist = Lista(0)
linkedlist.add_head(e1)
linkedlist.add_head(e2)
linkedlist.add_head(e3)
linkedlist.add_head(e4)
print(linkedlist.size)

